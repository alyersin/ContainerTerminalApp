<Window x:Class="ContainerTerminalApp.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ContainerTerminalApp.UI"
        mc:Ignorable="d"
        Title="Container Terminal App" Height="760" Width="900"
        WindowStartupLocation="CenterScreen"
        >
    <DockPanel>
        <ToolBarTray DockPanel.Dock="Top">
            <ToolBar Padding="10 0 0 0" >
                <Button Margin="0 0 16 0">File</Button>
                <Button Margin="0 0 16 0">Unit</Button>
                <Button Margin="0 0 16 0">Gate</Button>
                <Button Margin="0 0 16 0">Yard</Button>
                <Button Margin="0 0 16 0">Vessel</Button>
                <Button Margin="0 0 16 0">Rail</Button>
                <Button Margin="0 0 16 0">Reports</Button>
                <Button Margin="0 0 16 0">Windows</Button>
                <Button>Help</Button>
            </ToolBar>
        </ToolBarTray>



        <StackPanel>
            <Button>CLick me</Button>
        </StackPanel>
    </DockPanel>
</Window>
